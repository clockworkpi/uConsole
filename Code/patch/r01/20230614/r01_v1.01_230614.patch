+	/*pinctrl-1       = <&daudio2_pins_d>;*/
+	/* HDMI audio, no need pin */
+	pinctrl-0;
+	pinctrl-1;
+	pinctrl_used	= <0x0>;
+	daudio_type	= <0x1>;
+	status = "okay";
+};
